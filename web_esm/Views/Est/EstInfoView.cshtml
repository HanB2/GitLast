@model web_esm.Models_Act.Est.EstInfoModels

@{
	//팝업 형식 레이아웃 별도 지정
	Layout = "~/Views/Shared/_PopLayout.cshtml";
}


<div id="content">


	<div class="row">

		<!-- NEW WIDGET START -->
		<article class="col-sm-12 col-md-12 col-lg-12 sortable-grid ui-sortable">
			<!-- Widget ID (each widget will need unique ID)-->
			<div class="jarviswidget well" id="wid-id-3" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-togglebutton="false" data-widget-deletebutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-sortable="false" role="widget">
				<header role="heading">
					<span class="widget-icon"> <i class="fa fa-comments"></i> </span>
					<h2>Default Tabs with border </h2>

					<span class="jarviswidget-loader"><i class="fa fa-refresh fa-spin"></i></span>
				</header>
				<!-- widget div-->
				<div role="content">
					<div class="widget-body">
						<hr class="simple">
						<ul id="myTab1" class="nav nav-tabs bordered">
							<li class="active">
								<a href="#s1" data-toggle="tab" aria-expanded="true"> EST 기본정보 </a>
							</li>
							<li class="">
								<a href="#s2" data-toggle="tab" aria-expanded="true"> EST 계좌정보 </a>
							</li>
							<li class="">
								<a href="#s3" data-toggle="tab" aria-expanded="true"> 국가별 요율표 </a>
							</li>
							<li class="">
								<a href="#s4" data-toggle="tab" aria-expanded="true"> 계정 등급 관리 </a>
							</li>
							<li class="">
								<a href="#s5" data-toggle="tab" aria-expanded="true"> 계정 관리 </a>
							</li>
						</ul>
						<!--EST 통관비용 은 일단 생략-->

						<div id="myTabContent1" class="tab-content padding-10">
							<!--EST 기본정보-->
							<div class="tab-pane fade active in" id="s1">
								<iframe width="820px" height="800px;" scrolling="no" frameborder="0" id="viewIframe" src="/Est/EstIframeInfo?estCode=@Model.viewEstCode"></iframe>
							</div>
							<!--EST 계좌정보-->
							<div class="tab-pane fade" id="s2">
								<iframe width="820px" height="480px;" scrolling="no" frameborder="0" id="viewIframe" src="/Est/EstIframeAccount?estCode=@Model.viewEstCode"></iframe>
							</div>
							<!--국가별 요율표-->
							<div class="tab-pane fade" id="s3">
								<iframe width="820px" height="800px;" scrolling="yes" frameborder="0" id="viewIframe" src="/Est/EstIframeCostTable?estCode=@Model.viewEstCode"></iframe>
							</div>
							<!--계정 등급 관리-->
							<div class="tab-pane fade" id="s4">
								<iframe width="820px" height="800px;" scrolling="no" frameborder="0" id="viewIframe" src="/Est/EstIframeGrade?estCode=@Model.viewEstCode"></iframe>
							</div>
							<!--계정 관리 web_ese 게정관리_계정관리와 같음-->
							<div class="tab-pane fade" id="s5">
								<iframe width="820px" height="620px;" scrolling="no" frameborder="0" id="viewIframe" src="/Est/EstIframeUser?estCode=@Model.viewEstCode"></iframe>
							</div>

						</div>

					</div>
				</div>
			</div>
		</article>
	</div>




</div>